{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <oi@joaopedro.cc> (joaopedro.cc)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next -p 5120",
    "build": "next build",
    "build:no-cache": "run-s clean build",
    "start": "next start",
    "clean": "rimraf .next",
    "tsc": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint --fix --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "stylelint": "stylelint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx|scss|css)\" --write",
    "postinstall": "typesync",
    "storybook": "start-storybook -p 7007",
    "storybook:no-cache": "start-storybook --no-manager-cache -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "isomorphic-unfetch": "3.1.0",
    "next": "11.1.0",
    "next-themes": "0.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.38.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@tailwindcss/postcss7-compat": "2.2.7",
    "@types/autoprefixer": "^10.2.0",
    "@types/babel__core": "^7.1.15",
    "@types/eslint": "7.28.0",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/node": "16.7.1",
    "@types/prettier": "2.3.2",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/rimraf": "3.0.2",
    "@types/sass": "1.16.1",
    "@types/stylelint": "13.13.1",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "autoprefixer": "^10.3.2",
    "babel-loader": "^8.2.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-property-sort-order-smacss": "7.1.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.4.0",
    "stylelint-order": "4.1.0",
    "tailwindcss": "2.2.7",
    "typescript": "4.3.5",
    "typesync": "0.8.0"
  }
}
